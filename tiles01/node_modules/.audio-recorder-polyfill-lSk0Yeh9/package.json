    "edge",
    "safari",
    "wave",
    "wav",
    "webrtc",
    "audio",
    "web audio api",
    "recorder",
    "mp3"
  ],
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "ai/audio-recorder-polyfill",
  "type": "module",
  "main": "index.cjs",
  "module": "index.js",
  "react-native": "index.js",
  "exports": {
    ".": {
      "require": "./index.cjs",
      "import": "./index.js"
